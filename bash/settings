# Liferay paths

export LFR_PROJECTS_PATH="/Volumes/Projects"

export LFR_REPOS_PATH="$LFR_PROJECTS_PATH/repo"

# Exports
export ANT_AUTOCOMPLETE=/usr/local/etc/bash_completion.d/ant

export BASH_AUTOJUMP=`brew --prefix`/etc/autojump.sh

export BASH_COMPLETION_PATH=$(brew --prefix)/etc/bash_completion

# Git

export hd="HEAD"

export m="master"

# Config settings

# better tab complete
bind 'TAB:menu-complete'