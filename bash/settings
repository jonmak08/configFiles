# Liferay paths

export LFR_PROJECTS_PATH="/Volumes/Projects"

export LFR_REPOS_PATH="$LFR_PROJECTS_PATH/repo"

# Exports
export ANT_OPTS="-Xms512m -Xmx1024m -XX:MaxPermSize=512m"

export ANT_AUTOCOMPLETE=/usr/local/Cellar/ant/1.9.4/bin/complete-ant-cmd.pl

export BASH_AUTOJUMP=`brew --prefix`/etc/autojump.sh

export BASH_COMPLETION_PATH=$(brew --prefix)/etc/bash_completion

export EDITOR='subl -w'

export JAVA_HOME="$(/usr/libexec/java_home)"

export SMARTGITHG_MAX_HEAP_SIZE="4096m"

export PATH=$HOME/bin:/usr/local/bin:/usr/bin:/usr/sbin:/sbin:$HOME/configFiles/bin:$LFR_REPOS_PATH/git-tools:/usr/local/opt:/usr/local/share:$PATH

# Git

export hd="HEAD"

export m="master"

# Config settings

# better tab complete
bind 'TAB:menu-complete'